{
  "name": "oskari-frontend-hsy",
  "version": "1.0.0",
  "description": "These are HSY bundles for Oskari. They come with no official support from the core Oskari team.",
  "keywords": [
    "Oskari"
  ],
  "dependencies": {
    "acorn": "^6.1.1",
    "oskari-frontend": "https://git@github.com/oskariorg/oskari-frontend.git#1.56.0",
    "oskari-frontend-contrib": "https://git@github.com/oskariorg/oskari-frontend-contrib.git#1.56.0"
  },
  "scripts": {
    "dev-mode": "node ./node_modules/oskari-frontend/scripts/oskari-dev-mode",
    "dev-mode:off": "npm run dev-mode -- disabled",
    "build": "webpack --config ./node_modules/oskari-frontend/webpack.config.js --mode production",
    "start": "webpack-dev-server --config ./node_modules/oskari-frontend/webpack.config.js --hot --env.appdef=applications/hsy",
    "test": "eslint ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://bitbucket.sito.fi/scm/hsy/oskari-frontend-hsy.git"
  },
  "private": true,
  "devDependencies": {
    "eslint": "^4.6.1",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1"
  }
}
